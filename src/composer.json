{
    "name": "strident/trident-standard",
    "license": "MIT",
    "type": "project",
    "description": "The \"Trident Standard Edition\" distribution.",
    "autoload": {
        "psr-0": { "": "src/", "TridentStandard": "app/" }
    },
    "require": {
        "php": ">=5.5.3",
        "aptoma/twig-markdown": "0.2.1",
        "easybook/geshi": "dev-master",
        "michelf/php-markdown": "1.4.1",
        "raulfraile/ladybug": "v1.0.8",
        "strident/trident": "1.0.5-alpha3",
        "symfony/yaml": "2.5.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "trident-app-dir": "app",
        "trident-public-dir": "public"
    },
    "minimum-stability": "dev"
}
